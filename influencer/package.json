{
  "name": "influencer",
  "version": "1.0.0",
  "description": "Influencer",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "docker:build": "docker build -t influencer .",
    "docker:run": "docker run -p 3000:3000 -d influencer",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.14",
    "@loopback/context": "^1.6.0",
    "@loopback/core": "^1.1.7",
    "@loopback/openapi-v3": "^1.2.3",
    "@loopback/repository": "^1.1.7",
    "@loopback/rest": "^1.7.0",
    "@loopback/rest-explorer": "^1.1.10",
    "@loopback/service-proxy": "^1.0.9",
    "sequelize": "^5.8.9",
    "sequelize-cli": "^5.5.0",
    "sequelize-typescript": "^0.6.11"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.1",
    "@loopback/testlab": "^1.0.7",
    "@loopback/tslint-config": "^2.0.1",
    "@types/node": "^10.11.2",
    "cz-conventional-changelog": "^2.1.0",
    "typescript": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
