{
  "name": "influencer",
  "version": "1.0.0",
  "description": "Influencer",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js",
    "docker:build": "docker build -t influencer .",
    "docker:run": "docker run -p 3000:3000 -d influencer",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test",
    "migrate": "npm run clean && npm run build && node ./dist/console migrate:sequelize:sync"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.14",
    "@loopback/boot": "^1.0.10",
    "@loopback/context": "^1.4.1",
    "@loopback/core": "^1.1.4",
    "@loopback/openapi-v3": "^1.1.7",
    "@loopback/repository": "^1.1.3",
    "@loopback/rest": "^1.5.3",
    "@loopback/rest-explorer": "^1.1.6",
    "@loopback/service-proxy": "^1.0.6",
    "@types/bcrypt": "^3.0.0",
    "@types/multer": "^1.3.7",
    "@types/node-cron": "^2.0.1",
    "@types/sequelize": "^4.28.3",
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^6.2.0",
    "fs": "0.0.1-security",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.0",
    "lodash": "^4.17.11",
    "loopback-connector-mysql": "^5.3.1",
    "multer": "^1.4.1",
    "mysql2": "^1.6.5",
    "node-cron": "^2.0.3",
    "nodemon": "^1.18.10",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "qs": "^6.7.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.10",
    "sharp": "^0.22.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.1",
    "@loopback/testlab": "^1.0.7",
    "@loopback/tslint-config": "^2.0.1",
    "@types/node": "^10.11.2",
    "cz-conventional-changelog": "^2.1.0",
    "typescript": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
